  "license": "MIT",
  "bugs": {
    "url": "https://forum.bugsounet.fr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bugsounet/EXT-GooglePhotos"
  },
  "homepage": "https://wiki.bugsounet.fr/en/EXT-GooglePhotos",
  "dependencies": {
    "axios": "^1.6.8",
    "esbuild": "^0.20.2",
    "glob": "^10.3.12",
    "google-auth-library": "^9.7.0",
    "mkdirp": "^3.0.1",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
